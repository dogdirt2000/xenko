<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <SiliconStudioAssemblySign>true</SiliconStudioAssemblySign>
  </PropertyGroup>
  <Import Project="..\..\targets\Xenko.PreSettings.targets" />
  <PropertyGroup>
    <ProductVersion>8.0.30703</ProductVersion>
    <SchemaVersion>2.0</SchemaVersion>
    <ProjectGuid>{42780CBD-3FE7-48E3-BD5B-59945EA20137}</ProjectGuid>
    <OutputType>Library</OutputType>
    <RootNamespace>SiliconStudio.Xenko.Games</RootNamespace>
    <AssemblyName>SiliconStudio.Xenko.Games</AssemblyName>
    <SiliconStudioAssemblyProcessor>true</SiliconStudioAssemblyProcessor>
    <SiliconStudioAssemblyProcessorOptions>--auto-module-initializer</SiliconStudioAssemblyProcessorOptions>
    <SiliconStudioPlatformDependent>true</SiliconStudioPlatformDependent>
    <XenkoProjectPlatformDependentWindows>true</XenkoProjectPlatformDependentWindows>
    <XenkoBuildTags>*</XenkoBuildTags>
  </PropertyGroup>
  <Choose>
    <When Condition=" '$(AutoIncludeSystemAssembly)' != 'true'">
      <ItemGroup>
        <Reference Include="System" />
        <Reference Include="System.Core" />
      </ItemGroup>
    </When>
  </Choose>
  <Choose>
    <When Condition="$(SiliconStudioUI.Contains('SDL')) AND ('$(SiliconStudioPlatform)' != 'Linux')">
      <ItemGroup>
        <Reference Include="SDL2-CS, Version=0.0.0.0, Culture=neutral, processorArchitecture=MSIL">
          <SpecificVersion>False</SpecificVersion>
          <HintPath>$(XenkoDependenciesDir)\SDL2-CS\SDL2-CS.dll</HintPath>
        </Reference>
      </ItemGroup>
    </When>
    <When Condition="$(SiliconStudioUI.Contains('SDL')) AND ('$(SiliconStudioPlatform)' == 'Linux')">
      <ItemGroup>
        <Reference Include="SDL2-CS, Version=0.0.0.0, Culture=neutral, processorArchitecture=MSIL">
          <SpecificVersion>False</SpecificVersion>
          <HintPath>$(XenkoDependenciesDir)\SDL2-CS\Linux\SDL2-CS.dll</HintPath>
        </Reference>
      </ItemGroup>
    </When>
  </Choose>
  <Choose>
    <When Condition=" '$(XenkoGraphicsApi)' == 'Direct3D11' Or '$(XenkoGraphicsApi)' == 'Direct3D12' ">
      <ItemGroup>
        <Reference Include="SharpDX">
          <SpecificVersion>False</SpecificVersion>
          <HintPath>$(XenkoSharpDXSdkBinDir)\SharpDX.dll</HintPath>
        </Reference>
        <Reference Include="SharpDX.DXGI">
          <SpecificVersion>False</SpecificVersion>
          <HintPath>$(XenkoSharpDXSdkBinDir)\SharpDX.DXGI.dll</HintPath>
        </Reference>
      </ItemGroup>
    </When>
  </Choose>
  <Choose>
    <When Condition=" '$(XenkoGraphicsApi)' == 'Direct3D11' ">
      <ItemGroup>
        <Reference Include="SharpDX.Direct3D11" Condition=" '$(XenkoGraphicsApi)' == 'Direct3D11' ">
          <SpecificVersion>False</SpecificVersion>
          <HintPath>$(XenkoSharpDXSdkBinDir)\SharpDX.Direct3D11.dll</HintPath>
        </Reference>
      </ItemGroup>
    </When>
  </Choose>
  <Choose>
    <When Condition=" '$(XenkoGraphicsApi)' == 'Direct3D12' ">
      <ItemGroup>
        <Reference Include="SharpDX.Direct3D12" Condition=" '$(XenkoGraphicsApi)' == 'Direct3D12' ">
          <SpecificVersion>False</SpecificVersion>
          <HintPath>$(XenkoSharpDXSdkBinDir)\SharpDX.Direct3D12.dll</HintPath>
        </Reference>
      </ItemGroup>
    </When>
  </Choose>
