namespace SiliconStudio.Xenko.Rendering
{
    partial shader ParticleEffect
    {
        using params ParticleBaseKeys;

        mixin ParticleBaseEffect;

        mixin ParticleComputeColorShader;

        if (ParticleBaseKeys.BaseColor != null)
